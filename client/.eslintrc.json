{
  "extends": [
    "next/core-web-vitals",
    "airbnb-typescript",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "project": "./client/tsconfig.json"
  },
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
